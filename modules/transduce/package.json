{
  "name": "@yafu/transduce",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Ludwig Magnusson <ludwig.magnusson@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/cjs/transduce.cjs"
    },
    "./polyfills/*": {
      "import": "./lib/polyfills/*.js",
      "require": "./dist/cjs/polyfills/*.cjs"
    }
  },
  "main": "dist/cjs/transduce.js",
  "module": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint lib test *.js",
    "prepublishOnly": "yarn clean && yarn transpile",
    "pretest": "yarn",
    "test": "yarn unit",
    "transpile": "rollup -c",
    "unit": "mocha --esm"
  },
  "dependencies": {
    "yafu": "^1.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@yafu/eslint-config-yafu": "^1.0.2",
    "chai": "^4.2.0",
    "glob": "^7.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
